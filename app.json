{
  "expo": {
    "name": "Bandung.in",
    "slug": "bandung-in",
    "version": "1.0.0",
    "owner": "farellfaiz",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.farellfaiz.bandungin"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.farellfaiz.bandungin"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Bandung all-in-one application built with React Native.",
    "githubUrl": "https://github.com/farellfaiz/bandung.in"
  }
}
